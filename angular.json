{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xintra-tailwind": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:standalone": {
          "skipTests": true
        },
        "@schematics/angularirective": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angularipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angularervice": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "leaflet","apexcharts","sweetalert","sweetalert2","inputmask","filepond","moment","intl-tel-input","dropzone"
              ],
            "outputPath": "preview",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
                "src/.htaccess"
            ],
            "styles": [
              "src/styles.scss", "./node_modules/ngx-bar-rating/themes/br-default-theme.scss",
              "./node_modules/ngx-bar-rating/themes/br-square-theme.scss",
              "./node_modules/ngx-bar-rating/themes/br-horizontal-theme.scss",
              "./node_modules/ngx-bar-rating/themes/br-vertical-theme.scss",
              "./node_modules/ngx-bar-rating/themes/br-movie-theme.scss",
              "./node_modules/ngx-bar-rating/themes/br-stars-theme.scss",
               "node_modules/shepherd.js/dist/css/shepherd.css",
               "./node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": [ "node_modules/preline/dist/preline.js"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "xintra-tailwind:build:production"
            },
            "development": {
              "buildTarget": "xintra-tailwind:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": ["node_modules/preline/dist/preline.js"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
